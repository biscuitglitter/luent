{
  "compilerOptions": {
    "outDir": "dist",
    "incremental": true,
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "typeRoots": ["./custom_typings", "./node_modules/@types"],
  }
}
